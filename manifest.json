{
  "name": "YT Basic Views to Likes Ratio",
  "description": "Visualization of views in rel. to likes for a youtube video",
  "version": "1.0",
  "manifest_version": 3, 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      // "js": ["js-min_27-10-22.js", "content.js"]
      "js": ["js-min_27-10-22.js"],
      "css": ["main.css"]
    }
    // , {- same with "matches": ["https://amazon*"] etc.}
  ],
  
  //"content_security_policy": {"extension_pages": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self'"},  
  "background": {
      //"script": "js-min_27-10-22.js",
      "script": "https://code.jquery.com/jquery-latest.min.js",
    "service_worker": "main.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html"
  }
}